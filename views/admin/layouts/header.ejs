<script>
  document.addEventListener('DOMContentLoaded', () => {
    const uidDiv = document.getElementById('uid');
  const uid = uidDiv.textContent.trim();
    // Function to fetch complaint count
    function fetchComplaintCount() {
      // Assuming session is defined elsewhere in your code
     
     // console.log(uid);
      fetch('/countcomplain/?rowid=' + uid)
        .then(response => response.json())
        .then(data => {
          const countElement = document.getElementById('complaint-count');
          countElement.textContent = `${data.count}`;
        })
        .catch(error => {
          console.error('Error fetching complaint count:', error);
        });
    }

    // Fetch the complaint count every 10 seconds
    setInterval(fetchComplaintCount, 600000);

    // Fetch the complaint count when the page loads
    fetchComplaintCount();
  });
</script>
